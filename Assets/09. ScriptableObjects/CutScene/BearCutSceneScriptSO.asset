  cutSceneScriptList:
  - cutSceneLineList:
    - name: 
      lineText: "\uD30C\uB780 \uCD1D\uC54C 6\uAC1C\uC640\r \uBE68\uAC04 \uCD1D\uC54C
        3\uAC1C, \uADF8\uB9AC\uACE0 \uCD1D\uAE4C\uC9C0!"
    - name: 
      lineText: "\uD574\uB2F9 \uACF0\uB3CC\uC774 \uAC8C\uC784\uC5D0 \r\uCC38\uAC00\uD558\uC2E4
        \uC790\uACA9\uC774 \uCDA9\uBD84\uD569\uB2C8\uB2E4!"
    - name: 
      lineText: "\uD574\uB2F9 \uAC8C\uC784\uC758 \uB450 \uAC00\uC9C0 \uB8F0\uC744
        \uC124\uBA85\uB4DC\uB9AC\uACA0\uC2B5\uB2C8\uB2E4!"
  - cutSceneLineList:
    - name: 
      lineText: "\uCCAB\uBC88\uC9F8!\r"
    - name: 
      lineText: "\uBE68\uAC04\uC0C9 \uACF0\uB3CC\uC774\uB97C \uC81C\uC678\uD55C\r
        \uD30C\uB780\uC0C9 \uACF0\uB3CC\uC774\uB97C \uD574\uCE58\uC6CC\uB77C!"
  - cutSceneLineList:
    - name: 
      lineText: "\uD30C\uB780\uC0C9 \uACF0\uB3CC\uC774\uB4E4\uC740 \r\uCD1D\uC54C\uC744
        \uBB34\uC11C\uC6CC\uD574 \uBAA8\uB4E0 \uCD1D\uC54C\uC744 \uB9DE\uC73C\uBA74
        \uC544\uD30C\uD569\uB2C8\uB2E4."
  - cutSceneLineList:
    - name: 
      lineText: "\uD558\uC9C0\uB9CC \uBE68\uAC04\uC0C9 \uACF0\uB3CC\uC774\uB294 \uD30C\uB780\uC0C9
        \uCD1D\uC54C\uC815\uB3C4\uB294 \uBB34\uC11C\uC6CC\uD558\uC9C0 \uC54A\uAD70\uC694."
  - cutSceneLineList:
    - name: 
      lineText: "\uC557! \uADF8\uB798\uB3C4 \uC870\uC2EC\uD558\uC2ED\uC1FC. \uBE68\uAC04\uC0C9
        \uCD1D\uC54C\uC740 \uBE68\uAC04\uC0C9 \uACF0\uB3CC\uC774\uB3C4 \uC544\uC9C1
        \uBB34\uC11C\uC6CC\uD569\uB2C8\uB2E4!"
  - cutSceneLineList:
    - name: 
      lineText: "\uB450\uBC88\uC9F8!"
    - name: 
      lineText: "\uCD1D\uC54C\uC744 \uC790\uC720\uB86D\uAC8C \uBC30\uCE58\uD558\uC2ED\uC1FC!"
  - cutSceneLineList:
    - name: 
      lineText: "\uD574\uB2F9 \uCD1D\uC54C \uAD6C\uBA4D\uC5D0 \uBCF8\uC778\uC774
        \uC6D0\uD558\uB294 \uC0C9\uC758 \uCD1D\uC54C\uC744 \uB123\uC73C\uC2ED\uC1FC."
  - cutSceneLineList:
    - name: 
      lineText: "\uD558\uC9C0\uB9CC \uCD1D\uC54C\uC758 \uC2DC\uC791\uC740 \uC5B4\uB514\uBD80\uD130
        \uC2DC\uC791\uD560 \uC9C4 \uC81C\uC791\uC790\uC778 \uC800\uB3C4 \uBAA8\uB985\uB2C8\uB2E4."
  - cutSceneLineList:
    - name: 
      lineText: "\uD558\uC9C0\uB9CC \uAD1C\uCC2E\uC544\uC694! \uADF8 \uB2E4\uC74C\uBD80\uD130\uB294
        \r\uB0B4\uAC00 \uBC30\uCE58\uD55C \uC21C\uC11C\uB300\uB85C \uCD1D\uC54C\uC774
        \uB098\uAC00\uB2C8\uAE4C\uC694."
    - name: 
      lineText: "\uADF8\uB7EC\uB2C8 \uCD1D\uC54C \uBC30\uCE58\uB97C \uC2E0\uC911\uD788
        \uD558\uC154\uC57C\uACA0\uB124\uC694!"
  - cutSceneLineList:
    - name: 
      lineText: "\uC774\uC0C1! \uD574\uB2F9 \uAC8C\uC784\uC758 \uB8F0 \uC124\uBA85\uC744
        \uB9C8\uCE69\uB2C8\uB2E4."
    - name: 
      lineText: "\uACF0\uB3CC\uC774\uB4E4\uC744 \uC0B4\uB9AC\uAE30 \uC704\uD55C \uB9C9\uC911\uD55C
        \uCC45\uC784\uC744 \uAC00\uC9C4\r \uAC8C\uC784\uC785\uB2C8\uB2E4."
    - name: 
      lineText: "\uC131\uC2EC\uC131\uC758\uAECF \uC5F4\uC2EC\uD788 \uC784\uD574\uC8FC\uC138\uC694!"
